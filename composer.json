{
    "name": "annotatecms/packages",
    "type": "annotatecms-extension",
    "description": "Package management extension for AnnotateCms framework",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Michal Vyšinský",
            "email": "vysinsky@live.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "annotatecms/extensions-installer": "@dev",
        "annotatecms/framework": "@dev"
    },
    "suggest": {
        "annotatecms/themes": "Reveal full power of packages with themes package"
    },
    "autoload": {
        "psr-4": {
            "AnnotateCms\\Packages\\": "src/"
        }
    },
    "extra": {
        "config-dir": "app/config/",
        "extension-name": "packages",
        "extension-class": "AnnotateCms\\Packages\\DI\\PackagesExtension"
    }
}
